<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.text.Font?>

<Pane prefHeight="400.0" prefWidth="400.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.UserLoginController">
   <children>
      <Label layoutX="75.0" layoutY="61.0" prefHeight="53.0" prefWidth="251.0" text="Photo Library">
         <font>
            <Font name="System Bold" size="39.0" />
         </font>
      </Label>
      <Label layoutX="165.0" layoutY="156.0" text="LOGIN">
         <font>
            <Font size="24.0" />
         </font>
      </Label>
      <TextField fx:id="username" focusTraversable="false" layoutX="126.0" layoutY="200.0" prefHeight="30.0" prefWidth="149.0" promptText="Enter Username">
         <font>
            <Font size="16.0" />
         </font></TextField>
      <Button fx:id="login" layoutX="161.0" layoutY="249.0" mnemonicParsing="false" onAction="#handleButtonAction" prefHeight="25.0" prefWidth="78.0" text="Sign In">
         <font>
            <Font size="14.0" />
         </font>
      </Button>
   </children>
</Pane>
